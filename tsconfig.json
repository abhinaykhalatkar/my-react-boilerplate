{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],

    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@UI/*": ["Components/UI/*"],
      "@Util/*": ["Components/Util/*"],
      "@styles/*": ["styles/*"],
      "@Store/*": ["store/*"],
      "@Pages/*": ["Pages/*"],
      "@Assets/*": ["Assets/*"],
    },
  },
  "include": ["src/**/*.ts", "src/**/*.tsx", "src/global.d.ts"],
  "exclude": [
    "src/Components/UI//Editor",
    "src/Components/UI/AlertModule",
    "src/Components/UI/TopButton",
    // "src/Components/Backdrop"
  ]
}
