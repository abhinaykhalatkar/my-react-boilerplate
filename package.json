{
  "name": "abhinay-react-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.105",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "dompurify": "^3.1.6",
    "framer-motion": "^11.3.28",
    "quill": "2.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.3.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.26.1",
    "react-scripts": "5.0.1",
    "sass": "^1.72.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
  "start": "craco start",
  "build:ts": "tsc",
  "serve": "serve -s build",
  "build": "ts-node src/util/htaccessGen.ts && craco build && ts-node src/util/copyToBuild.ts",
  "prodBuild": "npm run tsxImportExe && npm run build",
  "eject": "react-scripts eject",
  "tsxImportExe": "ts-node --transpile-only -r ./src/util/register-ignore.js src/util/sitemapGen.ts && ts-node --transpile-only -r ./src/util/register-ignore.js src/util/roboTxtGen.ts"
},
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/css-modules": "^1.0.5",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^22.9.0",
    "@types/react-helmet-async": "^1.0.3",
    "image-webpack-loader": "^8.1.0",
    "responsive-loader": "^3.1.2",
    "serve": "^14.2.4",
    "sharp": "^0.33.5",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5"
  }
}
